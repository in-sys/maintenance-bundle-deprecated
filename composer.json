{
    "name": "in-sys/symfony-maintenance-bundle",
    "type": "symfony-bundle",
    "description": "This bundle allows you to place your website in maintenance mode by calling two commands from your console. Symfony 5.4 / 6",
    "homepage": "https://github.com/in-sys/symfony-maintenance-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "INSYS",
            "email": "contact@insys.email"
        },
        {
            "name": "Gilles Gauthier",
            "email": "g.gauthier@lexik.fr"
        },
        {
            "name": "Dev Lexik",
            "email": "dev@lexik.fr"
        },
        {
            "name": "Djuri Baars",
            "email": "info@djurict.nl"
        }
    ],
    "require": {
        "php": "^8.0||^8.1",
        "symfony/console": "^5.4||^6.0",
        "symfony/framework-bundle": "^5.4||^6.0",
        "symfony/translation": "^5.4||^6.0",
        "symfony/translation-contracts": "^2.0||^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*"
    },
    "autoload": {
        "psr-4": {
            "INSYS\\Bundle\\MaintenanceBundle\\": ""
        }
    },
    "scripts": {
        "unit-tests": [
            "vendor/bin/phpunit"
        ]
    }
}
